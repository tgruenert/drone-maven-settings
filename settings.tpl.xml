<?xml version="1.0" encoding="UTF-8" ?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<localRepository>${LOCAL_CACHE}</localRepository>
	<mirrors>
		<mirror>
			<id>central-proxy</id>
			<name>Local proxy of ${MIRROR:-central} repository</name>
			<url>${REPO_URL:-$RELEASES_URL}</url>
			<mirrorOf>${MIRROR:-central}</mirrorOf>
		</mirror>
	</mirrors>
	<profiles>
		<profile>
			<id>default-release</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<performRelease>false</performRelease>
				<deployAtEnd>true</deployAtEnd>
				<altReleaseDeploymentRepository>releases::${RELEASES_URL:-$REPO_URL}</altReleaseDeploymentRepository>
				<altSnapshotDeploymentRepository>snapshots::${SNAPSHOTS_URL:-$REPO_URL}</altSnapshotDeploymentRepository>
			</properties>
			<repositories>
				<repository>
					<id>releases</id>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<url>${RELEASES_URL:-$REPO_URL}</url>
				</repository>
				<repository>
					<id>snapshots</id>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
					<url>${SNAPSHOTS_URL:-$REPO_URL}</url>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>releases</id>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<url>${RELEASES_URL:-$REPO_URL}</url>
				</pluginRepository>
				<pluginRepository>
					<id>snapshots</id>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
					<url>${SNAPSHOTS_URL:-$REPO_URL}</url>
				</pluginRepository>
			</pluginRepositories>
		</profile>
		<profile>
			<id>properties</id>
			<activation>
				<activeByDefault>${PROPERTIES_ACTIVE:-true}</activeByDefault>
			</activation>
			<properties>$PROPERTIES</properties>
		</profile>
	</profiles>
	<servers>
		<server>
			<id>releases</id>
			<username>$USERNAME</username>
			<password>$PASSWORD</password>
		</server>
		<server>
			<id>snapshots</id>
			<username>$USERNAME</username>
			<password>$PASSWORD</password>
		</server>
		<server>
			<id>central-proxy</id>
			<username>$USERNAME</username>
			<password>$PASSWORD</password>
		</server>
		<server>
			<id>$DOCKER_REPO_URL</id>
			<username>$DOCKER_REPO_USERNAME</username>
			<password>$DOCKER_REPO_PASSWORD</password>
		</server>

	</servers>
</settings>
